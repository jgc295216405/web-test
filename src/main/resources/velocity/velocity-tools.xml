<?xml version="1.0" encoding="UTF-8"?>
<toolbox>
    <!--日期转换工具-->
    <tool>
        <key>date</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.DateTool</class>
        <parameter name="format" value="yyyy-MM-dd"/>
    </tool>
    <!--时间转换工具-->
    <tool>
        <key>dateTime</key>
        <scope>application</scope>
        <class>org.apache.velocity.tools.generic.DateTool</class>
        <parameter name="format" value="yyyy-MM-dd HH:mm:ss"/>
    </tool>
